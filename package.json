{
  "name": "svelte-domain",
  "version": "0.1.5",
  "description": "state manager of svelte",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "checktype": "tsc --noEmit",
    "test:watch": "vitest watch",
    "coverage": "vitest --coverage",
    "build": "vite build && tsc --emitDeclarationOnly"
  },
  "keywords": [
    "lib",
    "utils",
    "typescript",
    "svelte",
    "state"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thegenius/svelte-domain.git"
  },
  "author": {
    "name": "Wang Wei",
    "email": "thegenius@vip.qq.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.23",
    "typescript": "^4.8.4",
    "vite": "^3.1.4",
    "vitest": "^0.23.4"
  }
}
